@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@WebReportGlobals.Scripts()
@WebReportGlobals.Styles()

<div class="row">
    <div class="col-lg-12">
        <h2 class="page-header">
            <a href="@Url.Action("Details", "OS", new { id = ViewBag.Id})">
                <img src="~/Images/voltar.ico" title="Voltar" width="50px" height="50px" />
            </a>
            @ViewBag.Title
        </h2>
        <ol class="breadcrumb">
            <li><a href='@Url.Action("Index")'>Ordem de Serviço</a></li>
            <li><a href='@Url.Action("Details", new { id = ViewBag.Id})'>Detalhes</a></li>
            <li class="active">Relatório de Ordem de Serviço</li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        @ViewBag.Relatorio.GetHtml()
    </div>
</div>

